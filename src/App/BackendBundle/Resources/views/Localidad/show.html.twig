{% extends '::base.html.twig' %}

{% block body -%}
    <h1>Localidad</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Nombre_localidad</th>
                <td>{{ entity.nombrelocalidad }}</td>
            </tr>
            <tr>
                <th>Id_user_created_at</th>
                <td>{{ entity.idusercreatedat }}</td>
            </tr>
            <tr>
                <th>Created_at</th>
                <td>{{ entity.createdat|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
                <th>Id_user_updated_at</th>
                <td>{{ entity.iduserupdatedat }}</td>
            </tr>
            <tr>
                <th>Updated_at</th>
                <td>{{ entity.updatedat|date('Y-m-d H:i:s') }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('localidad') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('localidad_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
